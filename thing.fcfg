% start S

S -> WH O
S[SEM=('SELECT * FROM transactions' + ?q)] -> HM Q[SEM=?q]

WH -> 'What' 'is'
O -> 'a' 'CD'

HM -> 'How' 'much'

Q[SEM=''] -> 'have' 'I' 'spent'
Q[SEM=('WHERE' + ?q)] -> 'have' 'I' 'spent' 'on' TAGLIST[SEM=?q]

TAGLIST[SEM=('type=' + ?a)] -> TAG[NAM=?a]
TAGLIST[SEM=(?a + ' OR type=' + ?b)] -> TAGLIST[SEM=?a] 'and' TAG[NAM=?b]

TAG[NAM='frogs'] -> 'frogs'
TAG[NAM='coffee'] -> 'coffee'
